cd
tmux new -s 'notes' -n normal -d
tmux split-window -v -p 30 -t 'notes'
tmux split-window -h -p 70 -t 'notes'

tmux select-pane -t 'notes':1.1
tmux send-keys -t 'notes' 'nf' C-m
tmux select-pane -t 'notes':1.2
tmux clock-mode
tmux select-pane -t 'notes':1.3

tmux new-window -t 'notes' -n 'Notes'
tmux new-window -t 'notes' -n 'Meanings'

tmux select-window -t 'notes':2
tmux send-keys -t 'notes' 'cd ~/NOTES' C-m
tmux send-keys -t 'notes' 'clear' C-m
tmux send-keys -t 'notes' 'lss' C-m

tmux select-window -t 'notes':3
tmux send-keys -t 'notes' 'cd ~/NOTES/Vocabulary' C-m
tmux send-keys -t 'notes' 'clear' C-m
tmux send-keys -t 'notes' 'lss' C-m

tmux select-window -t 'notes':1
