cd ~/Desktop/learning/cs50\ Computer\ Science
tmux new -s 'cs50' -n normal -d
tmux split-window -v -p 30 -t 'cs50'
tmux split-window -h -p 70 -t 'cs50'

tmux select-pane -t 'cs50':1.1
tmux send-keys -t 'cs50' 'nf' C-m
tmux select-pane -t 'cs50':1.2
tmux clock-mode
tmux select-pane -t 'cs50':1.3
tmux send-keys -t 'cs50' 'lss' C-m

tmux new-window -t 'cs50' -n 'Editor'

tmux send-keys -t 'cs50' 'clear' C-m
tmux send-keys -t 'cs50' 'lss' C-m

tmux select-window -t 'cs50':2
