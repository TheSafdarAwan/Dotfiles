cd
tmux new -s 'tmux' -n testing -d
tmux split-window -v -p 30 -t 'tmux'
tmux split-window -h -p 70 -t 'tmux'

tmux select-pane -t 'tmux':1.1
tmux send-keys -t 'tmux' 'nf' C-m
tmux select-pane -t 'tmux':1.2
tmux clock-mode
tmux select-pane -t 'tmux':1.3

tmux new-window -t 'tmux' -n 'NOTES'
tmux new-window -t 'tmux' -n 'vimming'
tmux new-window -t 'tmux' -n 'scripting'

tmux select-window -t 'tmux':2
tmux send-keys -t 'tmux' 'cd ~/NOTES/tmux' C-m
tmux send-keys -t 'tmux' 'clear' C-m

tmux select-window -t 'tmux':3
tmux send-keys -t 'tmux' 'nvim ~/.tmux.conf' C-m

tmux select-window -t 'tmux':4
tmux send-keys -t 'tmux' 'cd ~/.dXDG/scripts/.local/bin' C-m
tmux send-keys -t 'tmux' 'clear' C-m
tmux send-keys -t 'tmux' 'lss' C-m

tmux select-window -t 'tmux':4
